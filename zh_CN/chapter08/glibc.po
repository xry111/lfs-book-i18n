"<literal># Begin /etc/nsswitch.conf\n"
"\n"
"passwd: files\n"
"group: files\n"
"shadow: files\n"
"\n"
"hosts: files dns\n"
"networks: files\n"
"\n"
"protocols: files\n"
"services: files\n"
"ethers: files\n"
"rpc: files\n"
"\n"
"# End /etc/nsswitch.conf</literal>\n"
"EOF</userinput>"
msgstr ""
"<userinput>cat &gt; /etc/nsswitch.conf &lt;&lt; \"EOF\"\n"
"<literal># Begin /etc/nsswitch.conf\n"
"\n"
"passwd: files\n"
"group: files\n"
"shadow: files\n"
"\n"
"hosts: files dns\n"
"networks: files\n"
"\n"
"protocols: files\n"
"services: files\n"
"ethers: files\n"
"rpc: files\n"
"\n"
"# End /etc/nsswitch.conf</literal>\n"
"EOF</userinput>"

#. type: Content of: <sect1><sect2><sect3><screen>
#: lfs-en/chapter08/glibc.xml
#, no-wrap
msgid ""
"<userinput>cat &gt; /etc/nsswitch.conf &lt;&lt; \"EOF\"\n"
"<literal># Begin /etc/nsswitch.conf\n"
"\n"
"passwd: files systemd\n"
"group: files systemd\n"
"shadow: files systemd\n"
"\n"
"hosts: mymachines resolve [!UNAVAIL=return] files myhostname dns\n"
"networks: files\n"
"\n"
"protocols: files\n"
"services: files\n"
