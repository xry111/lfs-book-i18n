#: /home/xry111/svn-repos/LFS-BOOK/chapter08/eudev.xml:55
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./configure --prefix=/usr           \\\n"
"            --bindir=/sbin          \\\n"
"            --sbindir=/sbin         \\\n"
"            --libdir=/usr/lib       \\\n"
"            --sysconfdir=/etc       \\\n"
"            --libexecdir=/lib       \\\n"
"            --with-rootprefix=      \\\n"
"            --with-rootlibdir=/lib  \\\n"
"            --enable-manpages       \\\n"
