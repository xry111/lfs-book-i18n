#. type: Content of: <sect1><sect2><screen>
#: /home/xry111/svn-repos/LFS-BOOK/chapter08/openssl.xml:48
#, no-wrap
msgid ""
"<userinput remap=\"configure\">./config --prefix=/usr         \\\n"
"         --openssldir=/etc/ssl \\\n"
"         --libdir=lib          \\\n"
"         shared                \\\n"
"         zlib-dynamic</userinput>"
msgstr ""
"<userinput remap=\"configure\">./config --prefix=/usr         \\\n"
"         --openssldir=/etc/ssl \\\n"
"         --libdir=lib          \\\n"
"         shared                \\\n"
"         zlib-dynamic</userinput>"

