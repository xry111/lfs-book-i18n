"            --disable-doxygen-docs               \\\n"
"            --disable-xml-docs                   \\\n"
"            --docdir=/usr/share/doc/dbus-&dbus-version;  \\\n"
"            --with-system-socket=/run/dbus/system_bus_socket</userinput>"

