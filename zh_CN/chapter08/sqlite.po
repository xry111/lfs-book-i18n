#: lfs-en/chapter08/sqlite.xml
msgid "Contents of Sqlite"
msgstr "Sqlite 的内容"

#. type: Content of: <sect1><sect2><variablelist><varlistentry><listitem><indexterm><primary>
#: lfs-en/chapter08/sqlite.xml
msgid "sqlite3"
msgstr "sqlite3"

